{
  "hosting": {
    "site": "flowstry-aaaa9",
    "public": "out",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/robots.txt",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/plain; charset=utf-8"
          }
        ]
      },
      {
        "source": "/sitemap.xml",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/xml; charset=utf-8"
          }
        ]
      }
      ,
      {
        "source": "/opengraph-image",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      },
      {
        "source": "/twitter-image",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/opengraph-image",
        "destination": "/opengraph-image.png"
      },
      {
        "source": "/twitter-image",
        "destination": "/twitter-image.png"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
